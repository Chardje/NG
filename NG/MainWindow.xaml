<Window x:Class="NG.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NG"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid x:Name="NGGrid" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image x:Name="BackgroundImage" Height="450" VerticalAlignment="Top" HorizontalAlignment="Center" Width="800"/>
        <Grid x:Name="DebugMenu" Visibility="Visible">
            <Label x:Name="FPS" Content="FPS:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="#FFE82121" Background="{x:Null}"/>
            <Label x:Name="NumofObj" Content="NofObj:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top" Foreground="#FFE82121" Background="{x:Null}"/>
            <Label x:Name="RendObj" Content="RendObj:" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top" Foreground="#FFE82121" Background="{x:Null}"/>

        </Grid>
    </Grid>
</Window>

